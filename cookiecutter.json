{
    "project_name": "Render Engine Plugin",
    "project_slug": "{{cookiecutter.project_name|lower|replace(' ', '_')|replace('-', '_')}}",
    "description": "A plugin for the Render Engine static site generator.",
    "default_settings": "{\"default_setting\": \"default_value\"}",
    "pre_build_site": true,
    "post_build_site": true,
    "render_content": true,
    "post_render_content": true,
    "pre_build_collection": true,
    "post_build_collection": true,
    "_copy_without_render": [
        "static",
        "content",
        "templates/_base.html",
        "templates/page.html",
        "templates/archive.html"
    ],
    "_jinja2_env_vars": {
        "lstrip_blocks": true,
        "trim_blocks": true
    }
}